   <property name="styleSheet">
    <string notr="true">/* Стиль для ToolBar */
QToolBar {
    border: none;
    border-bottom: 2px solid rgba(255, 255, 255, 0.15);
    spacing: 2px;
    padding: 2px;
	background-color: rgb(50,50,50);
}

/* Стиль для элементов ToolBar (кнопки) */
QToolButton {
    color: #e0e0e0;
    font-weight: 500;
    font-size: 13px;
    min-width: 32px;
    min-height: 32px;
    max-width: 32px;
    max-height: 32px;
    margin: 5px;
    border: none;  /* Убираем border по умолчанию */
    spacing: 5px;
    /* Выравнивание иконки */
    icon-size: 18px;
}

/* Hover состояние */
QToolButton:hover {
    background-color: rgba(255, 255, 255, 0.1);
    border: none;  /* Убираем border при hover */
    color: #ffffff;
}

/* Нажатое состояние - полностью убираем обводку */
QToolButton:pressed {
    background-color: rgba(255, 255, 255, 0.15);
    border: none;  /* Убираем border при нажатии */
    padding-top: 1px;
    padding-bottom: 0px;
    padding-left: 1px;
    padding-right: 0px;
}

/* Выбранное/активное состояние */
QToolButton:checked {
    background-color: rgba(0, 102, 204, 0.3);
    border: none;  /* Убираем border для checked состояния */
    color: white;
}

QToolButton:checked:hover {
    background-color: rgba(0, 102, 204, 0.5);
    border: none;  /* Убираем border для checked+hover */
}

/* Отключенное состояние */
QToolButton:disabled {
    background-color: rgba(30, 30, 30, 0.5);
    border: none;  /* Убираем border для disabled */
    color: rgba(255, 255, 255, 0.3);
}

/* Стиль для стрелки раскрывающегося меню */
QToolButton::menu-button {
    border: none;
    border-left: 1px solid rgba(255, 255, 255, 0.2);
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
    width: 16px;
    margin-left: 2px;
}

QToolButton::menu-button:hover {
    background-color: rgba(255, 255, 255, 0.1);
}

/* Стиль для кнопок с меню */
QToolButton[popupMode=&quot;1&quot;], QToolButton[popupMode=&quot;2&quot;] {
    border-radius: 8px;
    min-width: auto;
    max-width: none;
    min-height: 32px;
    max-height: 32px;
    padding: 0px 12px;
    padding-right: 16px;
    border: none;  /* Убираем border для кнопок с меню */
}

/* Стиль для иконок в тулбаре */
QToolBar QToolButton::icon {
    padding: 0px;
    width: 18px;
    height: 18px;
}

/* Стиль для разделителей в тулбаре */
QToolBar::separator {
    width: 1px;
    height: 24px;
    background-color: rgba(255, 255, 255, 0.2);
    margin: 4px 6px;
}

/* Стиль для выпадающих меню от кнопок тулбара */
QToolBar QMenu {
    background-color: rgba(40, 40, 40, 0.98);
    border: 1px solid rgba(255, 255, 255, 0.1);
    border-radius: 8px;
    padding: 6px 0px;
    font-size: 13px;
    color: #e0e0e0;
}

QToolBar QMenu::item {
    padding: 6px 24px 6px 16px;
    margin: 2px 6px;
    border-radius: 5px;
    color: #e0e0e0;
}

QToolBar QMenu::item:selected {
    background-color: #0066CC;
    color: white;
}

/* Вертикальный тулбар */
QToolBar:vertical {
    border-right: 1px solid rgba(255, 255, 255, 0.1);
    border-bottom: none;
}

QToolBar::handle:vertical {
    height: 4px;
    width: 20px;
    margin: 2px 4px;
}

QToolBar::separator:vertical {
    width: 20px;
    height: 2px;
    margin: 4px 2px;
}

/* Вертикальные кнопки в тулбаре */
QToolBar:vertical QToolButton {
    min-width: 32px;
    min-height: 32px;
    max-width: 32px;
    max-height: 32px;
    border-radius: 8px;
    border: none;  /* Убираем border для вертикальных кнопок */
}

/</string>
