    <string notr="true">
/* Стиль для Menu Bar - темная тема */
QMenuBar {
    background-color: rgba(40, 40, 40, 0.95);
    border: none;
    border-bottom: 1px solid rgba(50, 50, 50, 0.1);
    padding: 6px 10px;
    spacing: 8px;
    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial;
    font-size: 13px;
    font-weight: 500;
    color: #e0e0e0;
}

/* Стиль для элементов Menu Bar - круглые кнопки */
QMenuBar::item {
    background-color: rgba(30, 30, 30, 0.9);
    border: 1px solid rgba(100, 100, 100, 1);
    padding: 7px 13px;
    margin: 2px 2px;
    border-radius: 12px;  /* Увеличено с 2px до 15px для круглых кнопок */
    color: #e0e0e0;
    font-weight: 500;
}

/* Также можно увеличить радиус для других состояний */
QMenuBar::item:selected {
    background-color: rgba(255, 255, 255, 0.1);
    color: #ffffff;
    border-radius: 12px;  /* Добавлено для единообразия */
}

QMenuBar::item:pressed {
    background-color: rgba(255, 255, 255, 0.15);
    color: #ffffff;
    border-radius: 12px;  /* Добавлено для единообразия */
}

QMenuBar::item:open {
    background-color: rgba(40, 40, 40, 0.98);
    border: none;
    border-radius: 12px 12px 0 0;  /* Скруглены только верхние углы когда меню открыто */
}

/* Стиль для выпадающих меню - темная тема */
QMenu {
    background-color: rgba(40, 40, 40, 0.98);
    border: 1px solid rgba(255, 255, 255, 0.1);
    border-radius: 8px;
    padding: 6px 0px;
    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial;
    font-size: 13px;
    color: #e0e0e0;
    
    /* Тень для меню - более выраженная в темной теме */
    box-shadow: 0 8px 20px rgba(0, 0, 0, 0.5);
}

/* Стиль для пунктов меню */
QMenu::item {
    background-color: transparent;
    padding: 8px 24px 8px 16px;
    margin: 2px 6px;
    border-radius: 8px;
    color: #e0e0e0;
}

/* Стиль для выбранного пункта меню - синий акцент */
QMenu::item:selected {
    background-color: #0066CC;
    color: white;
    border-radius: 8px;
}

/* Стиль для недоступных пунктов */
QMenu::item:disabled {
    color: rgba(255, 255, 255, 0.3);
    background-color: transparent;
}

/* Стиль для разделителей */
QMenu::separator {
    height: 1px;
    background-color: rgba(255, 255, 255, 0.15);
    margin: 6px 12px;
}</string>
